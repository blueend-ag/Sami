{
    "output": "bin/sami.phar",
    "chmod": "0755",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php"
    ],
    "main": "sami.php",
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": ["*.php", "*.pem*"],
            "exclude": ["Tests", "tests"],
            "in": "vendor"
        },
        {
            "exclude": ["Tests"],
            "in": "Sami"
        }
    ],
    "compression": "NONE",
    "stub": false,
    "dump-autoload": false
}
